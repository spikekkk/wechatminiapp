.mine-title {
  position: fixed;
  width: 100%;
  top: 80rpx;
  font-size: 36rpx;
  color: #fff;
  font-weight: 600;
  z-index: 999999;
  text-align: center;
}

.movable-card {
  height: 512rpx;
