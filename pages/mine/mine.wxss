.card-item.center {
  text-align: center;
}

.card-item .card-bg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}

.card-item .title {
  color: #744a23;
  font-size: 40rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.movable-area .card-bg {
  width: 400%;
  height: 400%;
  transform: translate3d(0, 0, 0) scale(0.25, 0.25);
  transform-origin: left top;
}

.line-container {
  position: relative;
  margin-bottom: 20rpx;
}

.line-container .line {
  position: absolute;
  top: 50%;
  transform: translate(0, -50%);
  width: 222rpx;
  height: 2rpx;
  background-color: #5c523c;
}

.line-text {
  font-size: 20rpx;
  color: #5c523c;
  font-weight: 500;
}

.line-container .line.left {
  left: 0;
}

.line-container .line.right {
  right: 0;
}

.line-container .line .circle {
  width: 6rpx;
  height: 6rpx;
  border-radius: 50%;
  border: 2rpx solid #5c523c;
  top: -3rpx;
  position: absolute;
}

.line-container .line.left .circle {
  right: -9rpx;
}

.line-container .line.right .circle {
  left: -9rpx;
}

.member-rights {
  text-align: center;
}

.member-rights .rights-item {
  font-size: 24rpx;
  color: #5c523c;
  margin-bottom: 10rpx;
}

.member-rights .button {
  width: 368rpx;
  height: 72rpx;
  background: #373634;
  color: #ffe7a1;
  font-size: 28rpx;
  border-radius: 36rpx;
  line-height: 72rpx;
  margin: 20rpx auto;
  position: relative;
  z-index: 999;
}

.hasThisCard {
  text-align: left;
  padding: 40rpx 20rpx 0;
}

.hasThisCard .member-mess {
  padding-right: 100rpx;
  position: relative;
}

.hasThisCard .member-title {
  color: #744a23;
  font-size: 40rpx;
  margin-bottom: 20rpx;
}

.hasThisCard .expire-time {
  color: #5c523c;
  font-size: 20rpx;
}

.hasThisCard image {
  width: 148rpx;
  height: 148rpx;
  position: absolute;
  top: 10rpx;
  right: 10rpx;
}

.hasThisCard .rights {
  margin-top: 50rpx;
}

.hasThisCard .rights .rights-title {
  color: #5c523c;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.hasThisCard .rights .rights-item {
  color: #5c523c;
  font-size: 24rpx;
  margin-bottom: 10rpx;
}

.device {
  width: 100%;
  /* background-color: #fff; */
  margin-top: 230rpx;
}

.device .device-item {
  width: 720rpx;
  height: 88rpx;
  margin-left: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.device .detail-item:last-child {
  border-bottom: none;
}

.device .device-item .device-item-img {
  width: 36rpx;
  height: 36rpx;
}

.device .device-item .device-item-title {
  font-size: 32rpx;
  color: #333;
  margin-left: 20rpx;
}

.device .device-item .device-item-desc {
  font-size: 28rpx;
  color: #999;
  margin: auto 0 auto auto;
}

.device .device-item .device-item-btn {
  width: 16rpx;
  height: 16rpx;
  color: #c7c7cc;
  margin: auto 30rpx auto 20rpx;
  border: 3rpx #c7c7cc solid;
  border-top-color: transparent;
  border-left-color: transparent;
  transform: rotate(-45deg);
}

.play-card-layout {
  z-index: 100000000;
}

.playcard {
  width: 635rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: #fff;
  border-radius: 24rpx;
  z-index: 100000001;
}

.playcard-item {
  padding: 0 50rpx;
  border-bottom: 1rpx solid #979797;
}

.playcard-item-left {
  padding: 30rpx 0;
}

.playcard-item-left text {
  display: block;
}

.playcard-item-left .name {
  font-size: 40rpx;
  font-weight: 600;
}

.playcard-item-left .channel {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  font-size: 35rpx;
}

.playcard-item-left .time {
  font-size: 28rpx;
  color: #8a8a8a;
}

.playcard-item-right {
  width: 40rpx;
  height: 40rpx;
  background-color: #fff;
  border: 1px solid #eee;
  position: absolute;
  top: 70rpx;
  right: 30rpx;
  border-radius: 50%;
}

.animateToPlaycard .playcard-item-right {
  background-color: #999;
}

.customer-service {
