}

.container-content {
  position: absolute;
  top: 196rpx;
  z-index: 19;
  left: 50rpx;
}

.head {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}

.head .head-user {
  width: 120rpx;
  height: 120rpx;
  position: relative;
}

.head .head-user .user-avatar {
  width: 120rpx;
  height: 120rpx;
  border: 2px solid #fff;
  border-radius: 50%;
}

.head .head-user .levelImg {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: 0;
  right: 0;
  z-index: 99;
}

.head .head-info {
  display: flex;
  flex-direction: column;
  margin-left: 40rpx;
}

.head .head-btn {
  margin-left: 65px;
  width: 232rpx;
  height: 56rpx;
  background: rgba(55, 54, 52, 1);
  border-radius: 28rpx;
  text-align: center;
  line-height: 56rpx;
}

.head .head-btn text {
  color: #fff;
  font-weight: 550;
}

.head .head-info .user-name {
  margin-bottom: 20rpx;
  font-size: 32rpx;
  font-weight: 600;
  line-height: 32rpx;
  color: #fff;
  position: relative;
}

.head .head-info .user-cellphone {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #fff;
  font-weight: 200;
}

.movable-area {
  position: absolute;
  bottom: -200rpx;
  left: 0;
  height: 360rpx;
}

.movable-area {
  z-index: 99;
}

.movable-component {
  width: 100%;
  height: 360rpx;
  position: relative;
  z-index: 99;
}

.card-container {
  width: 100%;
  white-space: nowrap;
  padding: 0 72rpx;
}

.card-item {
  width: 556rpx;
  height: 360rpx;
  padding: 0 22rpx;
  border-radius: 20rpx;
  float: left;
  margin-right: 30rpx;
  position: relative;
  z-index: 999;
  box-shadow: 0 4rpx 12rpx 0px #502f0e;
  overflow: hidden;
}

