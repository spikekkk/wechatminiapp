}

.customer-service .cs-qrcode {
  width: 290rpx;
  height: 290rpx;
  padding: 10rpx;
  border-radius: 20rpx;
  background: rgba(196, 105, 222, 0.3);
  position: absolute;
  top: 55%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100000002;
}

.customer-service .cs-qrcode image {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.nick-icon {
  width: 71rpx;
  height: 32rpx;
}

.image-block {
  position: relative;
}

.platform-agreement {
  margin-top: 60rpx;
  margin-bottom: 60rpx;
  width: 100%;
  font-size: 24rpx;
